#include <iostream>
using namespace std;

void core(){
    int i, n, sum=0;

    cin >> n;

    int num[n];

    for(i=0; i<=n-1; i++){
        cin >> num[i];
    }

    for(i=0; i<=n-1; i++){
        sum = sum + num[i];
    }

    sum = sum / n;

    cout << endl << "---" << sum << "---" << endl;

}

int main(){

    core();

    return 0;
}
